<!doctype html>
<html lang="en">
<head>
    <title>Contact</title>
    <meta name="description" content="" />

    {{>head}}
</head>

<body>
    <div class="blocks-page">
		<section class="smart-scroll">
			<div class="container-fluid">
				<nav class="navbar navbar-expand-md navbar-light">
					<a class="navbar-brand heading-black" href="/">
						<img class="img-logo" src="{{{page.global.img_logo_nav}}}=s240"/>
					</a>
					<button class="navbar-toggler navbar-toggler-right border-0" type="button" data-toggle="collapse"
							data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false"
							aria-label="Toggle navigation">
						<i class="fa fa-bars"></i>
					</button>
					<div class="collapse navbar-collapse" id="navbarCollapse">
						<ul class="navbar-nav ml-auto mobile-nav">
							{{#page.global.header_nav.nav1.show_or_hide}}
							<li class="nav-item">
								<a class="nav-link" href="{{{page.global.header_nav.nav1.link}}}">{{page.global.header_nav.nav1.text}}</a>
							</li>
							{{/page.global.header_nav.nav1.show_or_hide}}

							{{#page.global.header_nav.nav2.show_or_hide}}
							<li class="nav-item">
								<a class="nav-link" href="{{{page.global.header_nav.nav2.link}}}">{{page.global.header_nav.nav2.text}}</a>
							</li>
							{{/page.global.header_nav.nav2.show_or_hide}}

							{{#page.global.header_nav.nav3.show_or_hide}}
							<li class="nav-item">
								<a class="nav-link" href="{{{page.global.header_nav.nav3.link}}}">{{page.global.header_nav.nav3.text}}</a>
							</li>
							{{/page.global.header_nav.nav3.show_or_hide}}
						</ul>
					</div>
				</nav>
			</div>
		</section>
		
		<section class="py-md-5 block feature-block" style ="background-image:url('{{{page.background}}}');background-size: cover">
			<div class="container">
				<div class="row block-header">
					<div class="col-sm-8 mx-auto text-center">
						<h2 class="display-4 mt-2">Contact Me</h2>
						<hr>
						<p class="lead">Feel free to contact me through these channels</p>
					</div>
				</div>
				<div class="row mt-5">
					<div class="col-6 col-lg-3">
						<a class="card card-body border text-center" href="#">
							<p class="feature-icon"><img src="{{{page.feature.img_1}}}=c" alt=""/></p>
							<h5 class="card-title text-dark mb-0">E-mail</h5>
							<p>anders.farr@gmail.com</p>
						</a>
					</div>
					<div class="col-6 col-lg-3">
						<a class="card card-body border text-center" href="#">
							<p class="feature-icon"><img src="{{{page.feature.img_2}}}=c" alt=""/></p>
							<h5 class="card-title text-dark mb-0">University E-mail</h5>
							<p>16ajjf@queensu.ca</p>
						</a>
					</div>
					<div class="col-6 col-lg-3">
						<a class="card card-body border text-center" href="#">
							<p class="feature-icon"><img src="{{{page.feature.img_3}}}=c" alt=""/></p>
							<h5 class="card-title text-dark mb-0">Phone Number</h5>
							<p>604-932-1321</p>
						</a>
					</div>
					<div class="col-6 col-lg-3">
						<a class="card card-body border text-center" href="https://www.linkedin.com/in/anders-farr/">
							<p class="feature-icon"><img src="{{{page.feature.img_4}}}=c" alt=""/></p>
							<h5 class="card-title text-dark mb-0">LinkedIn</h5>
							<p>Click Me!</p>
						</a>
					</div>
				</div>
			</div>
		</section>
		<footer class="pt-3">
			 <div class="container pt-5 pb-5">
				<div class="row">
					<div class="col-md-12 text-center">
						<p><img class="img-logo-footer" src="{{{page.global.img_logo_footer}}}=s440"/></p>

						<ul class="list-inline social social-white-alt social-rounded social-sm mb-0">
							
							{{#page.global.social.linkedin.show_or_hide}}
							<li class="list-inline-item"><a href="{{{page.global.social.linkedin.link}}}" target="_blank"><i class="fa fa-linkedin"></i></a></li>
							{{/page.global.social.linkedin.show_or_hide}}

						   
							{{#page.global.social.github.show_or_hide}}
							<li class="list-inline-item"><a href="{{{page.Sglobal.social.github.link}}}" target="_blank"><i class="fa fa-github"></i></a></li>
							{{/page.global.social.github.show_or_hide}}

						</ul>
					</div>
				</div>
			</div>
		</footer>
    </div>
	
	
</body>
</html>